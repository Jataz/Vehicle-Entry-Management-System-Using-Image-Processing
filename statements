CREATE TABLE vehicles (id INT NOT NULL AUTO_INCREMENT PRIMARY KEY, owner VARCHAR(255) NOT NULL, model VARCHAR(255), phone_number VARCHAR(255), color VARCHAR(255), role TEXT, status TEXT, number_plate VARCHAR(255));
CREATE TABLE users (userid VARCHAR(255), name VARCHAR(255), username VARCHAR(255), password VARCHAR(255));
